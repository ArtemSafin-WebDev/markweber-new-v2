<section class="intro js-intro-slider">
    <div class="intro__cursor">
        
    </div>
    <div class="intro__backgrounds">
        <div class="intro__background" style="background: linear-gradient(91.34deg, #13b17c 2.06%, #0099cc 100%)">

        </div>
        <div class="intro__background" style="background: linear-gradient(91.34deg, #632017 2.06%, #d8403d 100%)">

        </div>
